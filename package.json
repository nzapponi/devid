{
  "name": "devid",
  "version": "1.1.0",
  "description": "The most useful, developer friendly ID or token generator",
  "main": "./lib/cjs/devid.js",
  "module": "./lib/esm/devid.js",
  "exports": {
    ".": {
      "require": "./lib/cjs/devid.js",
      "default": "./lib/esm/devid.js"
    }
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "npm run lint && rm -rf lib && tsc && tsc -p tsconfig.cjs.json",
    "lint": "eslint --fix .",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "author": "Niccolo Zapponi <nzapponi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "crc": "^4.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "keywords": [
    "id",
    "uuid",
    "devid",
    "token"
  ]
}
